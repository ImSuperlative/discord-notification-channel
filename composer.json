{
    "name": "imsuperlative/discord-notification-channel",
    "description": "Laravel Notification Channel for laravel.",
    "version": "1.0",
    "keywords": [
        "laravel",
        "notifications",
        "Discord"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "imsuperlative",
            "email": "iamsuperlative@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "illuminate/notifications": "~5.7",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "nunomaduro/larastan": "^0.3.15"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Notifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminate\\Tests\\Notifications\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Illuminate\\Notifications\\DiscordChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
